<?xml version="1.0" encoding="utf-8" ?>
<ServiceManifest
  Name="PresenceLoadDriverServicePackage"
  Version="1.0"
  xmlns="http://schemas.microsoft.com/2011/01/fabric"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Description>Windows Fabric based calculator service.</Description>
    <ServiceTypes>
        <StatelessServiceType ServiceTypeName="PresenceLoadDriverServiceType" />
    </ServiceTypes>
    <CodePackage Name="Code" Version="1.0">
        <EntryPoint>
            <ExeHost>
                <Program>PresenceLoadDriverService.exe</Program>
              <ConsoleRedirection FileMaxSizeInKb="2048" FileRetentionCount="10"/>
            </ExeHost>
        </EntryPoint>
    </CodePackage>
    <ConfigPackage Name="Config" Version="1.0" />
    <Resources>
        <Endpoints>
            <Endpoint Name="PresenceLoadDriverServiceEndpoint" Protocol="tcp" />
        </Endpoints>
    </Resources>
</ServiceManifest>

