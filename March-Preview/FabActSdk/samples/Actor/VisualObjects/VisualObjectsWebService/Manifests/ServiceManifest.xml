<?xml version="1.0" encoding="UTF-8"?>
<ServiceManifest Name="VisualObjectsWebService" Version="1.0" xmlns="http://schemas.microsoft.com/2011/01/fabric" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Visual Objects Web Service</Description>
  <ServiceTypes>
    <StatelessServiceType ServiceTypeName="VisualObjectsWebService"></StatelessServiceType>
  </ServiceTypes>
  <CodePackage Name="Code" Version="1.0">
    <SetupEntryPoint>
      <ExeHost>
        <Program>VisualObjectsWebService.Setup.exe</Program>
        <ConsoleRedirection FileRetentionCount="5" FileMaxSizeInKb="128"/>
      </ExeHost>
    </SetupEntryPoint>
    <EntryPoint>
      <ExeHost>
        <Program>VisualObjectsWebServiceHost.exe</Program>
      </ExeHost>
    </EntryPoint>
  </CodePackage>
</ServiceManifest>
